{
    "extends":[
        "standard",
        "plugin:react/recommended",
        "plugin:tailwindcss/recommended",
        "prettier"
    ],
    "rules":{
        "max-len": [2,250],
        "no-multiple-empty-lines":[
            "error",
            {
                "max":1,
                "maxOf":1
            }
        ],
        "object-curly-newline":0
    }
}